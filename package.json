{
  "name": "zotero-navigate",
  "version": "0.0.1",
  "description": "Navigate to previous item selections with hotkeys in Zotero.",
  "scripts": {
    "lint": "eslint . --ext .ts --cache --cache-location .eslintcache/",
    "prebuild": "npm run lint",
    "build": "tsc --noEmit && node esbuild.js",
    "postbuild": "zotero-plugin-zipup build zotero-navigate",
    "release": "zotero-plugin-release",
    "postversion": "git push --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jdfinch/zotero-navigate.git"
  },
  "author": {
    "name": "jdfinch",
    "email": "jdfinch@emory.edu"
  },
  "bugs": {
    "url": "https://github.com/jdfinch/zotero-navigate/issues"
  },
  "homepage": "https://github.com/jdfinch/zotero-navigate",
  "dependencies": {
    "@types/mocha": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "esbuild": "^0.14.10",
    "eslint": "^8.6.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^37.5.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "mkdirp": "^1.0.4",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "zotero-plugin": "^1.0.62"
  },
  "xpi": {
    "name": "Navigate for Zotero",
    "updateLink": "https://github.com/jdfinch/zotero-navigate/releases/download/v{version}/zotero-navigate-{version}.xpi",
    "releaseURL": "https://github.com/jdfinch/zotero-navigate/releases/download/release/"
  }
}
